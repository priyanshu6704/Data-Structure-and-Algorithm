/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    private int count=0;
    private int res=-1;
    public int kthSmallest(TreeNode root, int k) {
       
         kSmall(root,k,count);
         return res;
    }
    public void kSmall(TreeNode root, int k,int index){
        if(root==null) return ;
        kSmall(root.left,k,count);
        count++;
        if(count==k){
            res=root.val;
            return;
        }
        kSmall(root.right,k,count);
        
    }
}
