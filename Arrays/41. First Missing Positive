class Solution {
    public int firstMissingPositive(int[] nums) {
        Set<Integer> set=new HashSet<>();
        Arrays.sort(nums);
        int l=nums[nums.length-1];
        for(int i=0;i<nums.length;i++){
            if(nums[i]>0){
                set.add(nums[i]);
            }
        }
        if(set.size()==0) return 1;
        int n=1;
        while(n<=l){
            if(!set.contains(n)){
                return n;
            }else{
                n++;
            }
        }
        return n;
    }
}
