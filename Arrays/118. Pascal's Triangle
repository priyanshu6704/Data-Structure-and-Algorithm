class Solution {
    public List<List<Integer>> generate(int numRows) {
        List<List<Integer>> list =new ArrayList<>();
        if(numRows==1){
            list.add(new ArrayList<>());
            list.get(0).add(1);
            return list;
        }
        for(int i=0;i<numRows;i++){
              list.add(new ArrayList<>());
              if(i==0){
                list.get(0).add(1);
              }
            for(int j=0;j<=i;j++){
                if(i==0){
                    break;
                }
              if(j==0 ){
                list.get(i).add(1);
              }
             else if(j==i){
                list.get(i).add(1);
              }else{
              int n1=list.get(i-1).get(j-1);
              int n2=list.get(i-1).get(j);
              list.get(i).add(n1+n2);
              }

            }
        }
        return list;
    }
}
