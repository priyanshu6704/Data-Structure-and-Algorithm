class Solution {
    public void rotate(int[][] matrix) {
        for(int i=0;i<matrix.length;i++){
            for(int j=i;j<matrix[0].length;j++){
                int temp=matrix[i][j];
                matrix[i][j]=matrix[j][i];
                matrix[j][i]=temp;
            }
        }
        for(int i=0;i<matrix.length;i++){
            int li=0;
            int hi=matrix[i].length-1;
            while(li<hi){
                int temp=matrix[i][li];
                matrix[i][li]=matrix[i][hi];
                matrix[i][hi]=temp;
                li++;
                hi--;
            }
        }
    }
}
